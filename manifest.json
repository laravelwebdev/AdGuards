{
    "manifest_version": 3,
    "name": "Ad Guards",
    "version": "3.5.1",
    "description": "Disable Ads and Pop Up",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "scripting",
        "webRequest",
        "storage"
    ],
    "host_permissions": [
        "https://www.geoguessr.com/"
    ],
    "content_scripts": [
        {
            "world": "MAIN",
            "matches": [
                "https://www.geoguessr.com/*"
            ],
            "js": [
                "popup-block.js"
            ],
            "all_frames": true,
            "run_at": "document_end"
        },
        {
            "world": "MAIN",
            "matches": [
                "https://www.worldguessr.com/*"
            ],
            "js": [
                "ads-block.js"
            ],
            "all_frames": true,
            "run_at": "document_end"
        }
    ]
}